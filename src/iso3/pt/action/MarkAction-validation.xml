<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC 
  		"-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
  		"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
  		
<validators>
	<field name="concept">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<!-- <message>Concept is required</message> -->
			<message key="errors.required.login.concept"/>
		</field-validator>
	</field>
	
	<field property="mark" depends="float">
        <arg0 key="mark"/>
    </field>
    
	<field name="mark">
		<field-validator type="required">
			<param name="trim">true</param>
			<message key="errors.required.login.password"/>
		</field-validator>
	</field>
</validators>

